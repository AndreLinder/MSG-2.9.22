<Application x:Class="MSG_by_AL__XAML_.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:MSG_by_AL__XAML_"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Dictionary.xaml"/>
                <ResourceDictionary x:Name="BACK" Source="Icons-XAML/Background.xaml"/>
                <ResourceDictionary x:Name="Chat" Source="Icons-XAML/Chat.xaml"/>
                <ResourceDictionary x:Name="Contact" Source="Icons-XAML/Contact.xaml"/>
                <ResourceDictionary x:Name="Logout" Source="Icons-XAML/Logout.xaml"/>
                <ResourceDictionary x:Name="Send" Source="Icons-XAML/Send.xaml"/>
                <ResourceDictionary x:Name="password" Source="Icons-XAML/password.xaml"/>
                <ResourceDictionary x:Name="file" Source="Icons-XAML/File.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
